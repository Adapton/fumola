import Seq "SeqTree";
module {
    public let inputArray = [
        (`1, 2),
        (`2, 7),
        (`3, 3),
        (`3-`1, 5),
        (`4, 6),
        (`5, 4),
        (`6, 13),
        (`7, 8),
        (`8, 1),
        (`9, 9),
    ];
    
    public func inputSeq () {
        let s = Seq.fromArray(inputArray);
        Seq.writeTikzText(`mergeSort-`inputSeq, s);
        s
    };

    public func output () = Seq.mergeSort(inputSeq());
}